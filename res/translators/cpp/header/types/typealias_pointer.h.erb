<%#
    Define every typealias as a pointer to an internal struct name
 %>
<%
  @typealiases.each do |typealias|
    internal_data_name = "struct _#{typealias[:name]}_data"
%>
<%= internal_data_name %>;
typealias struct <%= typealias[:name] %> *<%= internal_data_name %>;
<%
  end # end typealiases.each
%>
