// Dependencies
#include <stdlib.h>
#include <string>
<%
  if header_path.end_with? '.h'
%>
#include "<%= header_path %>"
<%
  else # else if header does not end with .h
%>
<%
    @data.each do |key, _|
%>
#include "<%= header_path %>/<%= SK_SRC_CORESDK %>/<%= key %>.h"
<%
    end # end data.each
%>
<%
  end # end if
%>
using std::string;

// Types
<%= read_template 'types/string' %>
<%= read_template 'types/bool' %>
<%= read_template 'types/direct' %>
<%= read_template 'types/typealias_pointer' %>
<%= read_template 'types/enum' %>
<%= read_template 'types/struct' %>
<%= read_template 'types/function_pointer' %>

// Function Prototypes
<%= read_template 'functions/prototypes' %>
