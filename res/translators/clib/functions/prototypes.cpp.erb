<%#
    Implement function prorotypes and expose as C
%>
<%
  @functions.each do |function|
%>
extern "C" <%= lib_signature_for(function) %>;
<%
  end # end functions.each
%>
<%#
    If responsible for freeing allocated heap memory, these functions
    need to be exposed (i.e., for every vector and every string)
%>
<%
  if free_heap_allocated?
%>
extern "C" void <%= func_prefix %>__free__sklib_string(__sklib_string s);
<%
    @vector_types.each do |type|
%>
extern "C" void <%= func_prefix %>__free__sklib_vector_<%= type %>(__sklib_vector_<%= type %> v);
<%
    end # end vector_types.each
%>
<%
  end # end if
%>
