<%#
    Enums are converted to their `int` representations using static casts.
 %>
<%
  @enums.pluck(:name).each do |enum|
%>
int <%= func_prefix %>__to_sklib_<%= enum %>(<%= enum %> v) {
    return static_cast<int>(v);
}
<%= enum %> <%= func_prefix %>__to_<%= enum %>(int v) {
    return static_cast<<%= enum %>>(v);
}
<%
  end # end enums.each
%>
