def __skadapter__to_sklib_char(v):
    if isinstance(v, str):
        return c_char(str.encode(v)[0])
    return c_char(v);

def __skadapter__to_char(v):
    return v.value

def __skadapter__to_sklib_unsigned_char(v):
    if isinstance(v, str):
        return c_ubyte(str.encode(v)[0])
    return c_ubyte(v);

def __skadapter__to_unsigned_char(v):
    return v.value
